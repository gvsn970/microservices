                       <!-- <option>Company One</option>
                        <option>Company Two</option>-->
		            </select>
                    <div class="input-group-btn">
                        <a href="#" onClick="pageRedirect();" class="btn btn-primary" style="margin-top:25px;" data-toggle="modal-demo" data-target="#custom-modal-create"> <i class="fa fa-plus"></i> </a>
                        
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group">
                    <label class="control-label pull-left">Prospect Email</label>
                    <input id="createprospectemail" type="email" class="form-control" placeholder="someone@company.com" required>
                  </div>
                </div>
              </div>
          
        <div class="row m-t-20">
          <div class="col-md-6 text-left">
            <button class="btn btn-primary waves-effect" onclick="addProspect();">Create</button>
            <button class="btn btn-default waves-effect" onclick="Custombox.close();">Cancel</button>
          </div>
          <span id="prospectErrorMessage"></span>
        </div>
      </div>
      <div class="col-md-12" id="addCompany" style="display:none;">
          
