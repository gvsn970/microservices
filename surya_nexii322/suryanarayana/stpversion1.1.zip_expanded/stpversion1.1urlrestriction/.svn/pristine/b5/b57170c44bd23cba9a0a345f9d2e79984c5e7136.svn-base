package com.nexiilabs.stp.resource;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "stp_requirement")
public class RequirementsModel {
		
		@Id
		@Column(name = "requirement_id")
		@GeneratedValue(strategy = GenerationType.IDENTITY)
		private int requirementId;
		@Column(name = "project_name")
		private String projectName;
		@Column(name = "project_description")
		private String projectDescription;
		@Column(name = "hm_name")
		private String hmName;
		@Column(name = "hm_email")
		private String hmEmail;
		@Column(name = "hm_contact")
		private String hmContact;
		@Column(name = "pm_name")
		private String pmName;
		@Column(name = "pm_email")
		private String pmEmail;
		@Column(name = "pm_contact")
		private String pmContact;
		@Column(name = "duration")
		private int duration;
		@Column(name = "experience_level")
		private String experienceLevel;
		@Column(name = "location")
		private String location;
		@Column(name = "skill_set")
		private String skillSet;
		@Column(name = "rate_card")
		private double rateCard;
		@Column(name = "created_by")
		private int createdBy;
		@Column(name = "fk_customer_id")
		private int fkCustomerId;
		@Column(name = "fk_employee_id")
		private int fkEmployeeId;
		@Column(name = "delete_status")
		private int deleteStatus;
		@Column(name = "updated_by")
		private int updatedBy;
		@Column(name = "deleted_by")
		private int deleteBy;
		
		public int getRequirementId() {
			return requirementId;
		}
		public void setRequirementId(int requirementId) {
			this.requirementId = requirementId;
		}
		
		public String getHmName() {
			return hmName;
		}
		public void setHmName(String hmName) {
			this.hmName = hmName;
		}
		public String getHmEmail() {
			return hmEmail;
		}
		public void setHmEmail(String hmEmail) {
			this.hmEmail = hmEmail;
		}
		public String getHmContact() {
			return hmContact;
		}
		public void setHmContact(String hmContact) {
			this.hmContact = hmContact;
		}
		public String getPmName() {
			return pmName;
		}
		public void setPmName(String pmName) {
			this.pmName = pmName;
		}
		public String getPmEmail() {
			return pmEmail;
		}
		public void setPmEmail(String pmEmail) {
			this.pmEmail = pmEmail;
		}
		public String getPmContact() {
			return pmContact;
		}
		public void setPmContact(String pmContact) {
			this.pmContact = pmContact;
		}
		public int getDuration() {
			return duration;
		}
		public void setDuration(int duration) {
			this.duration = duration;
		}
		public String getExperienceLevel() {
			return experienceLevel;
		}
		public void setExperienceLevel(String experienceLevel) {
			this.experienceLevel = experienceLevel;
		}
		public String getLocation() {
			return location;
		}
		public void setLocation(String location) {
			this.location = location;
		}
		public String getSkillSet() {
			return skillSet;
		}
		public void setSkillSet(String skillSet) {
			this.skillSet = skillSet;
		}
		public double getRateCard() {
			return rateCard;
		}
		public void setRateCard(double rateCard) {
			this.rateCard = rateCard;
		}
		public int getCreatedBy() {
			return createdBy;
		}
		public void setCreatedBy(int createdBy) {
			this.createdBy = createdBy;
		}
		public int getFkCustomerId() {
			return fkCustomerId;
		}
		public void setFkCustomerId(int fkCustomerId) {
			this.fkCustomerId = fkCustomerId;
		}
		public int getDeleteStatus() {
			return deleteStatus;
		}
		public void setDeleteStatus(int deleteStatus) {
			this.deleteStatus = deleteStatus;
		}
		public int getUpdatedBy() {
			return updatedBy;
		}
		public void setUpdatedBy(int updatedBy) {
			this.updatedBy = updatedBy;
		}
		public int getDeleteBy() {
			return deleteBy;
		}
		public void setDeleteBy(int deleteBy) {
			this.deleteBy = deleteBy;
		}
		
		public String getProjectName() {
			return projectName;
		}
		public void setProjectName(String projectName) {
			this.projectName = projectName;
		}
		public String getProjectDescription() {
			return projectDescription;
		}
		public void setProjectDescription(String projectDescription) {
			this.projectDescription = projectDescription;
		}
		
		public int getFkEmployeeId() {
			return fkEmployeeId;
		}
		public void setFkEmployeeId(int fkEmployeeId) {
			this.fkEmployeeId = fkEmployeeId;
		}
		@Override
		public String toString() {
			StringBuilder builder = new StringBuilder();
			builder.append("RequirementsModel [requirementId=");
			builder.append(requirementId);
			builder.append(", projectName=");
			builder.append(projectName);
			builder.append(", projectDescription=");
			builder.append(projectDescription);
			builder.append(", hmName=");
			builder.append(hmName);
			builder.append(", hmEmail=");
			builder.append(hmEmail);
			builder.append(", hmContact=");
			builder.append(hmContact);
			builder.append(", pmName=");
			builder.append(pmName);
			builder.append(", pmEmail=");
			builder.append(pmEmail);
			builder.append(", pmContact=");
			builder.append(pmContact);
			builder.append(", duration=");
			builder.append(duration);
			builder.append(", experienceLevel=");
			builder.append(experienceLevel);
			builder.append(", location=");
			builder.append(location);
			builder.append(", skillSet=");
			builder.append(skillSet);
			builder.append(", rateCard=");
			builder.append(rateCard);
			builder.append(", createdBy=");
			builder.append(createdBy);
			builder.append(", fkCustomerId=");
			builder.append(fkCustomerId);
			builder.append(", fkEmployeeId=");
			builder.append(fkEmployeeId);
			builder.append(", deleteStatus=");
			builder.append(deleteStatus);
			builder.append(", updatedBy=");
			builder.append(updatedBy);
			builder.append(", deleteBy=");
			builder.append(deleteBy);
			builder.append("]");
			return builder.toString();
		}
}
